package beans;

import java.io.Serializable;

public class NewBornID implements Serializable {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private String newBornID;
	
	public NewBornID(String theNewBornID) throws NullPointerException  {
		
		setNewBornID(theNewBornID);
	
	}
	
	/**
	 * @return the newBornID
	 */
	public String getNewBornID() {
		return newBornID;
	}

	/**
	 * @param theNewBornID the newBornID to set
	 */
	public void setNewBornID(String theNewBornID) {
		newBornID = theNewBornID;
	}

	public String toString() {

		String str = "";

		str = getNewBornID();
				
		return str;
	}

}
